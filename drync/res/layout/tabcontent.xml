<?xml version="1.0" encoding="utf-8"?>

<android.widget.DryncTabHost xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/drynctabhost" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
		<android.widget.DryncTabWidget android:id="@+id/tabs"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:layout_weight="0" />
		<FrameLayout android:id="@+id/tabcontent"
			android:layout_width="fill_parent" android:layout_height="0dip"
			android:layout_weight="1" />
	</LinearLayout>
</android.widget.DryncTabHost>